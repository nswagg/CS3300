<p id="notice"><%= notice %></p>

<div class="container">
  <div class="jumbotron">
    <h3>Project Title:</h3>
    <h1><%= @project.title %></h1>
    <span class="label label-primary"> Last update <%= @project.updated_at.strftime("%b %d, %Y") %></span>
  </div>
</div>
<div class="container">
<h3>Description:</h3>
  <div class="panel panel-default">
    <div class="panel-body">
      <p>
        <%=@project.description %>
      </p>
    </div>
  </div>
  <%= link_to 'Edit', edit_project_path(@project), class: 'btn btn-primary btn-sm' %>
  <%= link_to 'Back', projects_path, class: 'btn btn-primary btn-sm' %>
</div>